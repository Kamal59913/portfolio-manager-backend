generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id        Int      @id @default(autoincrement())
  name      String?  // Making this optional since your code doesn't set it
  email     String   @unique
  password  String
  createdAt DateTime @default(now()) // Optional, but good to have
}
